{
  "manifest_version": 3,
  "name": "Firebase access viewer",
  "description": "Visualize network requests to firebase services",
  "version": "1.0.1",
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png",
    "256": "icon256.png"
  },
  "devtools_page": "devtools.html",
  "action": {
    "default_icon": {
      "16": "icon16.png",
      "48": "icon48.png",
      "128": "icon128.png",
      "256": "icon256.png"
    },
    "default_popup": "popup.html"
  },
  "background": {
    "service_worker": "src/background.ts"
  }
}
